    "camelcase": "^6.2.0",
    "glob-promise": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@types/node": "^16.9.1",
    "babel-loader": "^8.2.2",
    "rimraf": "^3.0.2",
    "svgo": "^2.6.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "gitHead": "9aee86c05c5c90515ab4c6a9e92c6b39dd7d2d26"
